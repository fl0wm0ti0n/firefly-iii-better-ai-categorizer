{
  "name": "firefly-iii-ai-categorize",
  "version": "1.0.0",
  "description": "AI-powered transaction categorization for Firefly III with advanced features including drag & drop interface, auto-categorization, and word mappings",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 index.js",
    "dev:local": "nodemon index.js",
    "docker:build": "docker build -t firefly-ai-categorizer .",
    "docker:run": "docker-compose up -d",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "firefly-iii",
    "ai",
    "categorization",
    "openai",
    "finance",
    "automation",
    "drag-drop",
    "transaction-management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "openai": "^3.2.1",
    "queue": "^7.0.0",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
